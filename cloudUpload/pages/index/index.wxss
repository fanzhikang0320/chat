/* pages/index/index.wxss */
.container {
  width: 100%;
  height: 100%;
  background-color: rgb(61, 189, 248)
}
.row {
  width: 90%;
  background-color: white; 
  border-radius: 10rpx;
  padding: 20rpx 0;
  margin: 10rpx 0;
}

.row input {
  border: 2rpx solid #ccc;
  width: 90%;
  height: 60rpx;
  margin: 5rpx auto;
}
.row image {
  width: 100%;
  height: 300rpx;
}

