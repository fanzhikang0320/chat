/* pages/channelList/channelList.wxss */
.title {
  font-size: 40rpx;
  font-weight: bold;
}
.page-section {
  width: 100%;
  display: flex;
  flex-direction: column;
}
.row {
  position: relative;
  width: 100%;
  height: 100rpx;
  display: flex;
  flex-direction: row;
  justify-content: center;
  align-items: center;
  /* background-color: wheat; */
  border-bottom: 2rpx solid #ccc;
}
.row image {
  width: 80rpx;
  height: 80rpx;
  margin: 10rpx;
}
.swiper {
  width: 100%;
  height: 400rpx;
}
.swiper image {
  width: 100%;
  height: 100%;
}
.area {
  display: flex;
  flex-direction: column;
  margin: 10rpx;
}

.listRow {
  width: 95%;
  border-bottom: 2rpx solid #ccc;
  margin: 20rpx auto;
  padding-bottom: 10rpx;
}
.list button {
  margin-right: 10rpx;
  width: 180rpx;
  height: 80rpx;
  font-size: 30rpx;
  background-color: rgb(64, 250, 95);
  line-height: 64rpx;
}

navigator {
   width: 100%;
   height: 100%;
}


