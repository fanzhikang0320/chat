/* pages/index/index.wxss */
.page-section {
  width: 90%;
  display: flex;
  flex-direction: column;
  background-color: white;
  border-radius: 10rpx; 
  padding: 10rpx 0;
  margin: 4%;
}
.imgBox {
  width: 100%;
  height: 100%;
  display: flex;
  flex-direction: row;
  justify-content: flex-start;
}
.imgBox image {
  width: 40rpx;
  height: 40rpx;
  border-radius: 10rpx;
  margin: 5rpx;
}


