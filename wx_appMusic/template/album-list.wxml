<template name="album-list">
  <view class="big-photo-list">
    <view class="big-photo-item" wx:for="{{templateData}}">
      <image src="{{item.image}}" class="photo"></image>
      <text class="music-name">{{item.name}}</text>
      <text class="music-author">{{item.singer}}</text>
    </view>
  </view>
</template>