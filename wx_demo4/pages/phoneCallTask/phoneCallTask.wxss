/* pages/phoneCallTask/phoneCallTask.wxss */
.container {
  background-color: rgb(243, 221, 97);
}

.row {
  position: relative;
  height: 80rpx;
  width: 90%;
  border-radius: 10rpx;
  background-color: white;
  margin-bottom: 20rpx;
  padding-left: 20rpx;
  box-sizing: border-box;
  margin-top: 10rpx;
}
/* .yanCode {
  width: 70%;
} */
.row input {
  width: 100%;
  height: 100%;
}
.codeBtn {
  position: absolute;
  right: 0;
  top: 0;
  color: #ccc;
  width: 30%;
  height: 80rpx;
  line-height: 80rpx;
}

.subBtn {
  width: 200rpx;
  height: 80rpx;
  background-color: white;
  color: #333;
  border-radius: 50rpx;
  line-height: 80rpx;
}
